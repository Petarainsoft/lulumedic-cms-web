{
  "name": "lulumedic-cms-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "serve": "serve -s ./build",
    "start:demo": "dotenv -e .env.demo yarn start",
    "build:local": "dotenv -e .env.local yarn build",
    "build:demo": "dotenv -e .env.demo yarn build",
    "build:prod": "dotenv -e .env.production yarn build",
    "serve:demo": "yarn build:demo && serve -l 8520 -s ./build",
    "serve:prod": "yarn build:prod && serve -l 8520 -s ./build",
    "prerun:demo": "yarn install && yarn build:demo",
    "prerun:prod": "yarn install && yarn build:prod",
    "pm2:demo": "pm2 serve ./build 8520 --spa --name lulumedic-cms-web:demo",
    "pm2:prod": "pm2 serve ./build 8520 --spa --name lulumedic-cms-web:prod",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint ./src",
    "lint:migrateConfig": "npx @eslint/migrate-config .eslintrc.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/compat": "^1.2.6",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.4.3",
    "@mui/lab": "6.0.0-beta.26",
    "@mui/material": "^6.4.3",
    "@mui/x-data-grid": "^7.26.0",
    "@mui/x-date-pickers": "^7.26.0",
    "@types/jwt-decode": "^3.1.0",
    "dayjs": "^1.11.13",
    "dotenv-cli": "^8.0.0",
    "mui-image": "^1.0.7",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@types/eslint": "^9.6.1",
    "@types/mui-image": "^1",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run prettier:fix",
      "yarn run lint"
    ]
  }
}
